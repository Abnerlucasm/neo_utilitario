{
  "name": "neo_utilitario",
  "version": "1.0.0",
  "description": "Aplicação web para substituir o script .bat",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "create-menu-table": "node scripts/create-menu-table.js",
    "seed-menus": "node scripts/seed-menus.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:create": "sequelize-cli migration:create --name",
    "run-migrations": "node scripts/run-migrations.js",
    "check-associations": "node scripts/check-associations.js",
    "test-models": "node scripts/test-models.js",
    "fix-keywords": "node -e \"require('./migrations/fix-keywords-column.js').up(require('./config/database').sequelize.getQueryInterface(), require('sequelize'))\"",
    "convert-keywords": "node scripts/convert-keywords.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-ssh": "^13.2.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.5",
    "speakeasy": "^2.0.0",
    "ssh2": "^1.16.0",
    "umzug": "^3.7.2",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "ws": "^8.18.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.2"
  }
}
